@model WXSM.ViewModel.WX.ShopItemVMs.ShopItemListVM
@inject IStringLocalizer<WalkingTec.Mvvm.Core.Program> Localizer;

<wt:searchpanel vm="@Model" reset-btn="true">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Searcher.SearchTxt" empty-text="请输入类别,书号或买点进行查询" />
<wt:combobox field="Searcher.on_shelf" empty-text="@Localizer["All"]" />
</wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/WX/ShopItem/Search" />

